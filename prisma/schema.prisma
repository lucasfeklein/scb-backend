generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id                           Int       @id @default(autoincrement())
  email                        String    @unique
  emailVerified                Boolean   @default(false)
  emailVerificationToken       String    @default(uuid())
  emailVerificationTokenExpiry DateTime?
}
